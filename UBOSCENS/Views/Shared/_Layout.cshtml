    <script>
        $(document).ready(function (e) {
            console.log("Loading Counter");
            var num = 958855210;
            // initialise
            $('#counter').kCounter({
                height: 25,
                width: 25,
                easing: 'easeOutElastic'
            });
            $('#counter').kCounter({ initial: num, duration: 1000 });
            setInterval(function () {

                $('#counter').kCounter('update', num++);
            }, 1000);
            // $('.selectpicker').selectpicker();
        })
    </script>
