
@{
    ViewBag.Title = "AddToTable";
}

<h2>AddToTable</h2>

@using (Html.BeginForm("AddTable", "DataImport", FormMethod.Post,
                         new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Add Tables to the table</h4>

        <div class="form-group">
            <label class="control-label col-md-2">Story Image</label>
            <div class="col-md-10">
                <input type="file" class="multi" name="file" id="file" multiple />

            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

